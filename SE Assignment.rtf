/* Style The Dropdown Button */\par
.dropbtn \{\par
  background-color: #4CAF50;\par
  color: white;\par
  padding: 16px;\par
  font-size: 16px;\par
  border: none;\par
  cursor: pointer;\par
\}\par
\par
/* The container <div> - needed to position the dropdown content */\par
.dropdown \{\par
  position: relative;\par
  display: inline-block;\par
\}\par
\par
/* Dropdown Content (Hidden by Default) */\par
.dropdown-content \{\par
  display: none;\par
  position: absolute;\par
  background-color: #f9f9f9;\par
  min-width: 160px;\par
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);\par
  z-index: 1;\par
\}\par
\par
/* Links inside the dropdown */\par
.dropdown-content a \{\par
  color: black;\par
  padding: 12px 16px;\par
  text-decoration: none;\par
  display: block;\par
\}\par
\par
/* Change color of dropdown links on hover */\par
.dropdown-content a:hover \{background-color: #f1f1f1\}\par
\par
/* Show the dropdown menu on hover */\par
.dropdown:hover .dropdown-content \{\par
  display: block;\par
\}\par
\par
/* Change the background color of the dropdown button when the dropdown content is shown */\par
